{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "0.2.1",
  "default_locale": "es",
  "description": "__MSG_extDesc__",
  "icons": {
    "16": "img/icon_16.jpg",
    "48": "img/icon_48.jpg",
    "128": "img/icon_128.jpg"
  },
  "author": "LCApps",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "default-src 'self' data: https://api.openweathermap.org/ https://source.unsplash.com/ https://images.unsplash.com/ https://antc-rss-json.herokuapp.com https://api.rss2json.com ; img-src 'self' data: https: http: ; style-src 'unsafe-inline' 'self' data: https: http: ",
  "current_locale": "es",
  "externally_connectable": {
    "matches": [
      "https://api.openweathermap.org/*",
      "https://*.unsplash.com/*",
      "http://*.programmingexcuses.com/*",
      "https://api.rss2json.com/*",
      "https://antc-rss-json.herokuapp.com/*"
    ]
  },
  "incognito": "split",
  "offline_enabled": true,
  "optional_permissions": [
    "tabs",
    "geolocation"
  ],
  "permissions": [
    "geolocation",
    "storage",
    "unlimitedStorage"
  ],
  "short_name": "__MSG_extName__",
  "version_name": "Alpha 0.2.1"
}